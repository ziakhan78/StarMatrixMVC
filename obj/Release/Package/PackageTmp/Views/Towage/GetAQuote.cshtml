@model StarMatrix.Models.Towage
@{
    ViewData["Title"] = "Get a Quote";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<section class="inner_banner" style="position: relative;">

    <div class="overlay_inn">
        <div class="container"><h3>Lorem Ipsum is simply dummy text of the printing and typesetting industry. </h3></div>
    </div>
    <img src="~/images/Slider-01.jpg" alt="Slider" class="img-fluid" />

</section>

<div class="bg-dark ">
    <div class="container">
        <div class="row">

            <nav class=" ">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="~/Home">Home</a></li>
                    <li class="breadcrumb-item"><a href="~/Towage/">Towage</a></li>
                    <li class="breadcrumb-item active">Get a Quote</li>
                </ol>
            </nav>



        </div>
    </div>
</div>

<div class="clearfix"></div>



<section class="container mt-3">
    <div class="row">

        <div class="col-md-9">
            <h2> Get a Quote </h2>

            <p class="text-justify">
                Lorem Ipsum is simply dummy text of the printing and typesetting industry Lorem Ipsum is simply dummy text of the printing and typesetting industry Lorem Ipsum is simply dummy text of the dummy text of the printing and industry
            </p>

            <form asp-action="Create">
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="DeparturePort" class="control-label"></label>
                    <input asp-for="DeparturePort" class="form-control" />
                    <span asp-validation-for="DeparturePort" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="DestinationPort" class="control-label"></label>
                    <input asp-for="DestinationPort" class="form-control" />
                    <span asp-validation-for="DestinationPort" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="EstimatedDisplacement" class="control-label"></label>
                    <input asp-for="EstimatedDisplacement" class="form-control" />
                    <span asp-validation-for="EstimatedDisplacement" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="TowingDraft" class="control-label"></label>
                    <input asp-for="TowingDraft" class="form-control" />
                    <span asp-validation-for="TowingDraft" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Dimensions" class="control-label"></label>
                    <input asp-for="Dimensions" class="form-control" placeholder="Dimensions (L x B x W)" />
                    <span asp-validation-for="Dimensions" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="EstimatedLaycan" class="control-label"></label>
                    <input asp-for="EstimatedLaycan" class="form-control" />
                    <span asp-validation-for="EstimatedLaycan" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="TypeofTow" class="control-label"></label>
                    <input asp-for="TypeofTow" class="form-control" placeholder="(Jack up Rig, Semi Submersible, Ship etc) Please specify" />
                    <span asp-validation-for="TypeofTow" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <label asp-for="MinimumBPRequirements" class="control-label"></label>
                    <input asp-for="MinimumBPRequirements" class="form-control" />
                    <span asp-validation-for="MinimumBPRequirements" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <input type="submit" value="Submit" class="btn btn-primary" />
                </div>
            </form>


        </div>

        <div class="col-md-3 wow fadeInUp" data-wow-delay="0.3s">

            <div class="list-group margin_top ">
                <a href="~/Towage/" class="list-group-item bg-light right_paneldhy mb-2"> Present Fleet</a>
                <a href="~/Towage/Live-Location" class="list-group-item bg-light right_paneldhy mb-2"> Live Location</a>
                <a href="~/Towage/Availability" class="list-group-item bg-light right_paneldhy mb-2"> Availability</a>
                <a href="~/Towage/Get-A-Quote" class="list-group-item bg-light right_paneldhy mb-2"> Get a Quote</a>
                <a href="~/Towage/Jobs-Undertaken" class="list-group-item bg-light right_paneldhy mb-2"> Jobs Undertaken</a>
            </div>
        </div>







    </div>


</section>


